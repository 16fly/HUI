    "semantic-release": "^17.1.1",
    "semantic-release-cli": "^5.3.1",
    "standard-version": "^8.0.2",
    "tailwindcss": "^1.4.6",
    "validate-commit-msg": "^2.14.0",
    "vue-docgen-api": "^4.26.0",
    "vue-docgen-loader": "^1.5.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2"
