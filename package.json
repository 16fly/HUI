    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "sass-loader": "^9.0.2",
    "storybook": "^5.3.19",
    "style-loader": "^1.2.1",
    "vue": "^2.6.11"
