{
  "name": "@huanet/hui",
  "version": "0.0.0-development",
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HuaNet/HUI.git"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "start-storybook": "start-storybook -s ./public",
